{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "env": {
          "GOOGLE_SERVICES_FILE": "{{ GOOGLE_SERVICES_PLIST }}"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
